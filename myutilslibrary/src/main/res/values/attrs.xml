<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="RxScaleImageView">
        <attr name="src" format="reference" />
        <attr name="assetName" format="string" />
        <attr name="panEnabled" format="boolean" />
        <attr name="zoomEnabled" format="boolean" />
        <attr name="quickScaleEnabled" format="boolean" />
        <attr name="tileBackgroundColor" format="color" />
    </declare-styleable>

    <declare-styleable name="ExpandTextView">
        <attr name="expandText" format="string" />
        <attr name="collospeText" format="string" />
        <attr name="expandTextColor" format="color" />
        <attr name="collospeTextColor" format="color" />
        <attr name="showedLineCount" format="integer" />
    </declare-styleable>
</resources>